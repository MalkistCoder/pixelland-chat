    "events": "^3.3.0",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "linkify-string": "^4.0.2",
    "pinia": "^2.0.28",
    "tw-elements": "^1.0.0-alpha13",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue3-markdown-it": "^1.0.10",
    "vue3-perfect-scrollbar": "^1.6.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.2",
    "@graphql-codegen/client-preset": "^1.2.4",
