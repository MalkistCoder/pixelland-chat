{
  "name": "pixelland-chat",
  "version": "0.0.0",
  "description": "Discord-style embedded chat used in PixelLand",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wwwillw/pixelland-chat"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "stage": "vite --host --mode staging",
    "build": "vue-tsc --noEmit && vite build --base=/src/chat/dist/",
    "codegen": "graphql-codegen --config codegen.yml",
    "preview": "vite preview"
